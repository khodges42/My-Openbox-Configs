<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                 file:///usr/share/openbox/menu.xsd">
	<menu id="root-menu" label="Openbox 3">
		<item label="pr0ns">
			<action name="Execute">
				<execute>x-www-browser</execute>
			</action>
		</item>
		<item label="hax">
			<action name="Execute">
				<execute>terminator</execute>
			</action>
		</item>
		<item label="subl">
			<action name="Execute">
				<execute>subl</execute>
			</action>
		</item>
		<item label="music">
			<action name="Execute">
				<execute>terminator -l music</execute>
			</action>
		</item>
		<item label="install gentoo">
			<action name="Execute">
				<execute>virtualbox</execute>
			</action>
		</item>
		<item label="tmux">
			<action name="Execute">
				<execute>tmux</execute>
			</action>
		</item>
		<!-- This requires the presence of the 'menu' package to work -->
		<menu id="/Debian"/>
		<separator/>
		<menu id="client-list-menu"/>
		<separator/>
		<item label="ObConf">
			<action name="Execute">
				<execute>obconf</execute>
			</action>
		</item>
		<item label="Reconfigure">
			<action name="Reconfigure"/>
		</item>
		<item label="Restart">
			<action name="Restart"/>
		</item>
		<separator/>
		<item label="Exit">
			<action name="Exit"/>
		</item>
	</menu>
</openbox_menu>
